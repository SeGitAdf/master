{
	"name": "DataFlowtest_1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Se_Sqddb_Demo",
						"type": "DatasetReference"
					},
					"name": "sqldb"
				},
				{
					"dataset": {
						"referenceName": "DS_Se_Sqddb_Demo",
						"type": "DatasetReference"
					},
					"name": "sqldb2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          studentId as integer,",
				"          name as string,",
				"          surname as string,",
				"          birthdate as date,",
				"          gender as string,",
				"          class as string,",
				"          point as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     batchSize: 10,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sqldb",
				"source(output(",
				"          studentId as integer,",
				"          name as string,",
				"          surname as string,",
				"          birthdate as date,",
				"          gender as string,",
				"          class as string,",
				"          point as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     batchSize: 10,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sqldb2",
				"sqldb, sqldb2 join(sqldb@studentId == sqldb2@studentId,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}